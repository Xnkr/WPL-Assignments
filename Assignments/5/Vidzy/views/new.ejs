<%- include('partials/header')%>

<form action="/videos" method="POST">

<div class="form-group">
<label for "mytitle"> Title </label>
<input type="text" name="title" id="mytitle" class="form-control">
</div>

<div class="form-group">
<label for "mygenre"> Genre </label>
<input type="text" name="genre" id="mygenre" class="form-control">
</div>

<div class="form-group">
<label for "myimage"> Image </label>
<input type="text" name="image" id="myimage" class="form-control">
</div>

<div class="form-group">
<label for "mydesc"> Description </label>
<textarea name="desc" id="mydesc" class="form-control"> </textarea>
</div>

<div class="form-group">
<button type="submit" class="btn btn-info"> Save </button>
</div>
</form>

<a href="/"> Cancel </a>
</div>

<%- include('partials/footer')%>